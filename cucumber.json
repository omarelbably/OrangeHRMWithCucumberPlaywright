{
    "default":{
        "paths":["cucumber/features"],
        "formatOptions":{
            "snippetInterface":"async-await"
        },
        "require":["cucumber/steps/*.ts", "cucumber/hooks/hooks.ts"],
        "requireModule":["ts-node/register"],
        "parallel":1,
        "retry":0,
        "format":["html:cucumber/report/cucumber-report.html", "json:cucumber/report/cucumber-report.json"]
    }
}